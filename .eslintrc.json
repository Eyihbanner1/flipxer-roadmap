{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "globals": {
    "document": "readonly",
    "window": "readonly",
    "localStorage": "readonly",
    "location": "readonly",
    "console": "readonly",
    "setTimeout": "readonly",
    "confirm": "readonly",
    "Blob": "readonly",
    "URL": "readonly"
  },
  "rules": {
    "no-unused-vars": ["warn", { "varsIgnorePattern": "^(exportProgress|importProgress|resetProgress)$" }]
  }
}
